{
    "manifest_version": 2,
    "name": "Background Image",
    "description": "Extension that shows images from popular cinematics",
    "version": "1.0",
    "author": "Nikolaj Jensen",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Nice backgrounds"
    },
    "chrome_url_overrides": {  "newtab": "newtab.html"},
    "permissions": ["activeTab"]
}